                                </a>
                                <p class="card-text">{{truncateText this.Meta_description 100}}</p>
                                <!-- Mô tả bài viết -->
                                <p class="date">
                                    {{formatDate this.Date}} <!-- Ngày bài viết đầu tiên -->
                                </p>
                            </div>
                        </div>
                        {{/each}}
                    </div>


                </div>

                <div class="carousel-item">
                    <div class="item">
                        {{#each secondNewsget12TopViewedNews}}
                        <div class="card" style="width: 18rem;" href="/news/{{this.Id_News}}">
                            <img src="/imgs/home/bv.jpg" class="card-img-top" alt="...">
                            <div class="card-body">
                                <a href="#">
                                    <h5 class="card-title">{{truncateText this.Meta_title 30}}</h5>
                                    <!-- Tiêu đề bài viết -->
                                </a>
                                <p class="card-text">{{truncateText this.Meta_description 100}}</p>
                                <!-- Mô tả bài viết -->
                                <p class="date">
                                    {{formatDate this.Date}} <!-- Ngày bài viết đầu tiên -->
                                </p>
                            </div>
                        </div>
                        {{/each}}
                    </div>


                </div>

                <div class="carousel-item">
                    <div class="item">
                        {{#each thirdNewsget12TopViewedNews}}
                        <div class="card" style="width: 18rem;" href="/news/{{this.Id_News}}">
                            <img src="/imgs/home/bv.jpg" class="card-img-top" alt="...">
                            <div class="card-body">

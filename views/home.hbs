<div class="card-body pr-0 pl-0" id="body">
    <!-- Tin tức nổi bật & top 10 categories -->
    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-8">
                <div class="row">
                    {{#if firstNewstopViewedNews}}
                    <div class="grid-head" href="/news/{{firstNewstopViewedNews.Id_News}}">
                        <img src="/imgs/home/donal.jpg"
                            class="img-grid-head img-fluid ${3|rounded-top,rounded-right,rounded-bottom,rounded-left,rounded-circle,|}"
                            alt="">
                        <div class="content-grid-head">
                            <h4>{{truncateText firstNewstopViewedNews.Meta_title 30}}</h4>
                            <!-- Tiêu đề bài viết đầu tiên -->
                            <p>{{truncateText firstNewstopViewedNews.Meta_description 100}}</p>
                            <!-- Mô tả bài viết đầu tiên -->
                            <p class="date">
                                {{formatDate firstNewstopViewedNews.Date}} <!-- Ngày bài viết đầu tiên -->
                            </p>
                        </div>
                    </div>
                    {{/if}}
                </div>
                <div id="article_list">
                    <!-- Hiển thị 3 bài viết tiếp theo -->
                    {{#each News234stopViewedNews}}
                    <div class="article-list-element" href="/news/{{this.Id_News}}">
                        <img src="/imgs/home/bv.jpg" class="img-fluid" alt="">
                        <div class="article-list-content">
                            <a href="#">
                                <h5>{{truncateText this.Meta_title 30}}</h5> <!-- Tiêu đề bài viết -->
                            </a>
                            <p>{{truncateText this.Meta_description 100}}</p> <!-- Mô tả bài viết -->
                            <p class="date">
                                {{formatDate this.Date}} <!-- Ngày bài viết -->
                            </p>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>


            <div class="col-sm-4">
                <div class="card mb-3 shadow-sm" id="article_top10">
                    <div class="list-group list-group-flush">
                        {{#each topCategories}}
                        <div class="card-body" href="/news/{{this.latestNews.Id_News}}">
                            <div class="category">{{this.CategoryName}}</div>
                            <a href="#">
                                <h6 class="card-title">{{truncateText this.latestNews.Meta_title 30}}</h6>
                                <!-- Tiêu đề bài viết mới nhất -->
                            </a>
                            <p class="card-text">{{truncateText this.latestNews.Meta_description 100}}</p>
                            <!-- Mô tả bài viết -->
                            <div class="date">
                                <div>{{formatDate this.latestNews.Date}}</div> <!-- Ngày bài viết -->
                            </div>
                        </div>
                        {{/each}}
                    </div>
                </div>
            </div>


        </div>
    </div>






    <!-- 12 news view cao nhất mọi danh mục trong 1 tháng -->
    <div class="container-fluid" id="article_top_high">
        <div class="title">
            <h4>12 news view cao nhất mọi danh mục trong 1 tháng </h4>
        </div>
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">


            <div class="carousel-inner">
                <div class="carousel-item active">
                    <div class="item">
                        {{#each firstNewsget12TopViewedNews}}
                        <div class="card" style="width: 18rem;" href="/news/{{this.Id_News}}">
                            <img src="/imgs/home/bv.jpg" class="card-img-top" alt="...">

                                <p class="card-text">{{truncateText this.Meta_description 100}}</p>
                                <!-- Mô tả bài viết -->
                                <p class="date">
                                    {{formatDate this.Date}} <!-- Ngày bài viết đầu tiên -->
                                </p>
                            </div>
                        </div>
                        {{/each}}
                    </div>


                </div>

            </div>



            <button class="carousel-control-prev btn-secondary" type="button" data-target="#carouselExampleControls"
                data-slide="prev">
                <span class="carousel-control-prev-icon " aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </button>
            <button class="carousel-control-next btn-secondary" type="button" data-target="#carouselExampleControls"
                data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </button>
        </div>


    </div>








